<Window x:Class="FinalProject.SimulationElements.Dialog.FutureEventListViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FutureEventListViewer" Height="408" Width="330" DataContext="{Binding RelativeSource={RelativeSource Self}}">
    <Grid>
        <ListView Name="LstViewData" ItemsSource="{Binding GetData }">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Time" Width="150" DisplayMemberBinding="{Binding Time}" />
                    <GridViewColumn Header="Event" Width="150" DisplayMemberBinding="{Binding Event}" />
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
