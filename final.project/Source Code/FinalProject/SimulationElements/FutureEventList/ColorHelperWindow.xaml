            <Rectangle Fill="Silver" HorizontalAlignment="Left" Height="26" Margin="184,48,0,0" Stroke="Black" VerticalAlignment="Top" Width="26"/>
            <Rectangle Fill="YellowGreen" HorizontalAlignment="Left" Height="26" Margin="184,79,0,0" Stroke="Black" VerticalAlignment="Top" Width="26"/>
