            <Label Content="Empty" HorizontalAlignment="Left" Margin="136,202,0,0" VerticalAlignment="Top"/>
            <Label Content="Not Empty" HorizontalAlignment="Left" Margin="104,233,0,0" VerticalAlignment="Top"/>
